{
  "name": "twenti-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:expose": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "npx prettier --ignore-path .gitignore --check \"**/*.+(js|jsx|json|ts|tsx|html|css)\"",
    "format:fix": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|html|css)\"",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prepare": "husky install",
    "validate": "npm run lint && npm run format && vitest --no-watch"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.1",
    "eslint": "^8.27.0",
    "eslint-plugin-monk": "^0.0.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1"
  }
}
